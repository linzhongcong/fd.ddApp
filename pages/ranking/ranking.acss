
/* ---------------------------排行榜模块------------------- */
.ranking-wrap {
  background-color: #f5f5f5;
  width: 100vw;
  height: 100vh;
  overflow-y: auto;
}

.ranking-wrap  .tab-wrap {
  overflow: hidden;
  text-align: center;
  border-radius: 0 0 24rpx 24rpx;
  padding: 30rpx;
  height: auto;
  background-color: #fff;
}

.ranking-wrap .tabs {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  width: 100%;
  font-size: 28rpx;
  color: #999;
}

.tabs .diligent, .performance {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50%;
  height: 80rpx;
  background: #f5f5f5;
  border-radius: 0rpx 10rpx 10rpx 0;
}

.tabs .actived {
  background-color: #00a1e9;
  border-radius: 10rpx 0 0 10rpx;
  color: #fff;
}

.tabs .pactived {
  background-color: #00a1e9;
  border-radius: 0 10rpx 10rpx 0;
  color: #fff;
}

.tabs-performance {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  margin-bottom: -8rpx;
  color: #999;
}

.tabs-performance 
.tabs-total,
.tabs-newers {
  width: 50%;
  padding: 24rpx 0;
  padding-bottom: 0px;
  font-size: 28rpx;
  font-weight: 400;
}

.tabs-performance 
text:not(.totalActived):not(.newerActived) {
  padding-left: 1rem;
  padding-right: 1rem;
}

.tabs-performance 
.totalActived, .newerActived {
  color: #00a1e9;
  border-bottom: 4rpx solid #00a1e9;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 16rpx;
}

.ranking-content-wrap {
  padding: 30rpx;
  padding-bottom: 20rpx;
}

.ranking-content-wrap .content-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 48rpx;
  height: 120rpx;
  overflow: hidden;
  background-color: #fff;
  border-radius: 10rpx;
}

.content-header .username {
  font-size: 32rpx;
  font-weight: bold;
  color: #666;
}

.content-header .user-msg {
  display: flex;
  justify-content: space-between;
}

.ranking-type {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 82rpx;
  color: #333;
  font-weight: 500;
  font-size: 26rpx;
  border-right: 2rpx solid #ddd;
  /* padding-right: 20rpx; */
  overflow: hidden;
}

.ranking-text 
.ranking-type:last-child {
  border: none;
}

.detail-header .detail-list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 88rpx;
  padding: 0rpx 50rpx;
  color: #666;
  background-color: #fff;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
}

.detail-header .ranking-text {
  background-color: #f5f5f5;
}
.detail-list .ranking-num {
  text-align: center;
  max-width: 25%;
  overflow: hidden;
}

.detail-list .ranking-member {
  display: flex;
  justify-content: center;
  align-content: center;
  position: relative;
  width: 25%;
  text-align: center;
}

.detail-list .isNew::after {
  position: absolute;
  top: -20rpx;
  right:-30rpx;
  content: "NEW";
  display: flex;
  justify-content: center;
  align-items: center;
  width: 40rpx;
  height: auto;
  padding: 4rpx;
  color: #fff;
  font-size: 16rpx;
  background-color: #F22208;
  border-radius: 120rpx 100rpx 100rpx 0;
} 

.detail-list .sign-time {
  text-align: center;
  width: 170rpx;
}

.detail-list .sign-count {
  text-align: center;
  width: 120rpx;
}

.ranking-wrap .ranking-view-nomore {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-bottom: 20rpx;
  color: #ccc;
  font-size: 20rpx;
  font-weight: 400;
  background-color: #f5f5f5;
}


.content-header {
  background-color: #fff;
  background-size:  20% auto;
  background-position: left;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,)
}